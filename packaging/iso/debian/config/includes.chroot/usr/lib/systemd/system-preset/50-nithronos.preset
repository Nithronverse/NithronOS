# NithronOS systemd preset - Lower priority (50) than /etc presets (90)
# Core services that must run for NithronOS to function
enable nos-agent.service
enable nosd.service
enable nithronos-otp-gen.service
enable nithronos-issue-gen.service
enable nithronos-runtime-enable.service

# Services enabled conditionally or later
disable caddy.service
disable nos-tls-gen.service
disable docker.service


